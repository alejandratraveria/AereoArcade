{
	"name": "aereoarcade",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile build/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile build/smui-dark.css -i src/theme/dark",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@smui/button": "^7.0.0-beta.8",
		"@smui/fab": "^7.0.0-beta.8",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"smui-theme": "^7.0.0-beta.8",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.0"
	},
	"type": "module",
	"dependencies": {
		"@smui/layout-grid": "^7.0.0-beta.8",
		"svelte-material-ui": "^7.0.0-beta.8"
	}
}
